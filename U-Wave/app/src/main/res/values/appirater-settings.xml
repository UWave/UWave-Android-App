<?xml version="1.0" encoding="utf-8"?>
<resources>
<!-- Change the market_url for other markets i.e. Amazon App Store
    Google Play = market://details?id=%s
    Amazon App Store = http://www.amazon.com/gp/mas/dl/android?p=%s
 -->
<string name="appirator_app_title">the UWave Radio App</string>
<string name="appirator_rate_title">Rate %s</string>
<string name="appirator_message">Enjoying %s? If so, please rate us in the Play Store! Thanks!</string>
<string name="appirator_market_url" translatable="false">market://details</string>
<integer name="appirator_days_until_prompt">30</integer>
<integer name="appirator_launches_until_prompt">15</integer>
<integer name="appirator_untimed_events_until_prompt">15</integer>
<integer name="appirator_timed_events_until_prompt">15</integer>
<string name="appirator_button_rate">Yes, Rate %s</string>
<string name="appirator_button_rate_later">Remind me later</string>
<string name="appirator_button_rate_cancel">No, Thanks</string>
<!-- Set this to 0 to remind every launch -->
<integer name="appirator_days_before_reminding">3</integer>
<!-- Change to true to show every time, for testing -->
<bool name="appirator_test_mode">false</bool>
<!-- Change modify button colors -->
<color name="appirator_button_start_color">#ff449def</color>
<color name="appirator_button_end_color">#ff2f6699</color>
<color name="appirator_button_text_color">#ffffffff</color>
<!-- Change title color -->
<color name="appirator_title_color">#ff449def</color>
</resources>

